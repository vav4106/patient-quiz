sub_marks=[]
for i in range (6):
    sub_marks.append(float(input(f"Enter the marks of subject{i+1}:")))
def bubble_sort(arr):
    n=len(arr)
    for i in range(n-1):
        for j in range(n-i-1):
            if arr[j]<arr[j+1]:
                arr[j],arr[j+1]=arr[j+1],arr[j]
    return arr
print("Sorted(Descending):",bubble_sort(sub_marks.copy()))
print("marks=",sub_marks)