my_list=[1,2,3,4,5]
print("initial list:",my_list)
my_list.insert(3,44)
print("After inserting 44 at index 3:",my_list)
my_list.remove(4)
print("after removing 4 from the list:",my_list)
my_list.append(6)
print("after appending 6:",my_list)
print("length of the list:",len(my_list))
my_list.pop()
print("List after popping :",my_list)
my_list.clear()
print("after clearing the list:",my_list)