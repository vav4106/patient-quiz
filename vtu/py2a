n=int(input("Enter a number:"))
n1=0
n2=1
count=0
if n<0:
    print("Enter a positive integer,number entered is invalid")
elif n==1:
    print("the fibonacci series number upto ",n)
    print(n1)
else:
    print("The fibonacci sequence of the numbers are:")
    while count<n:
        print(n1)
        nth=n1+n2
        n1=n2
        n2=nth
        count=count+1