n=int(input("Enter a number:"))
n1=0
n2=1
count=0
if (n<0):
    print("Enter valid choice, it is invalid:")
elif (n==1):
    print("The fibonacci series upto",n)
    print(n1)
else:
    print("the fibonacci sequence of numbers:")
    while (count<n):
        print(n1)
        nth=n1+n2
        n1=n2
        n2=nth
        count+=1