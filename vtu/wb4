def factorial(n):   
    if n == 0:
        return 1 
    return n * factorial(n-1)
N = int(input("Enter N:"))
R=int(input("Enter R:"))
if N>R:
  bin=factorial(N)/(factorial(R)*factorial(N-R))
print("Factorial of", R, "is", factorial(R))
print("Factorial of", N, "is", factorial(N))
print("Binomial of", N , R ,"is", bin)
