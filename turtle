def remove_occurrences():
    # Taking list input from user
    lst = list(map(int, input("Enter elements of the list separated by space: ").split()))
    
    # Element to remove
    elem = int(input("Enter the element to remove: "))

    # Removing all occurrences
    result = [x for x in lst if x != elem]

    print("List after removing all occurrences:", result)


# Call the function
remove_occurrences()
